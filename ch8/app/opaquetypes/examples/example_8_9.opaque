module ints-1
    interface [opaque t
            zero : t
            succ : (t -> t)
            pred : (t -> t)
            is-zero : (t -> bool)]  
    body [type t = int
                    zero = 0
                    succ = proc(x : t) -(x,-5)
                    pred = proc(x : t) -(x,5)
                    is-zero = proc (x : t) zero?(x)]

let zero = from ints-1 take zero
in let succ = from ints-1 take succ
in (succ (succ zero))