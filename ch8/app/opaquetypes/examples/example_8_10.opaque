module ints-2
    interface [opaque t
            zero : t
            succ : (t -> t)
            pred : (t -> t)
            is-zero : (t -> bool)]  
    body [type t = int
                    zero = 0
                    succ = proc(x : t) -(x,3)
                    pred = proc(x : t) -(x,-3)
                    is-zero = proc (x : t) zero?(x)]

let z = from ints-2 take zero
in let s = from ints-2 take succ
in (s (s z))